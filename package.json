{
  "name": "sosgame",
  "version": "0.1.5",
  "author": "Alex Sav",
  "description": "Sos game",
  "private": true,
  "main": "index.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "ws": "node devtools/ws-server.js",
    "start": "webpack serve --config webpack_configs/webpack.config.js --mode development",
    "all": "npm-run-all -p -r ws start",
    "android": "webpack --config webpack_configs/android.webpack.config.js --mode production",
    "build": "webpack --config webpack_configs/production.webpack.config.js --mode production",
    "style": "npx eslint . -c linters/eslint.config.js",
    "style:fix": "npx eslint . -c linters/eslint.config.js --fix",
    "style2": "npx stylelint --config linters/.stylelintrc.json \"src/**/*.css\"",
    "style2:fix": "npx stylelint --config linters/.stylelintrc.json \"src/**/*.css\" --fix",
    "style3": "npx oxlint -D all -A no-console -A max-params -A prefer-add-event-listener -A no-useless-undefined -A no-undefined -A no-async-await -A func-names -A default-case -A filename-case -A no-lonely-if -A sort-keys -A no-magic-numbers -A no-plusplus -A prefer-global-this -A id-length -A yoda -A func-style -A switch-case-braces -A max-lines-per-function -A init-declarations -A prefer-set-has --threads=1 --deny-warnings  --ignore-pattern=src/js/lib/* ./src",
    "test": "node --test --test-reporter dot",
    "test:verbose": "node --test",
    "test:coverage": "npx c8 node --test --test-reporter dot",
    "updates": "npx ncu",
    "updates:fix": "npx ncu -u && npm install"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.1.0",
    "c8": "^10.1.3",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "html-inline-css-webpack-plugin": "^1.11.2",
    "html-webpack-plugin": "^5.6.3",
    "jsdom": "^26.1.0",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-check-updates": "^18.0.1",
    "npm-run-all": "^4.1.5",
    "oxlint": "^1.5.0",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "workbox-webpack-plugin": "^7.3.0",
    "ws": "8.18.3"
  },
  "dependencies": {
    "@akamfoad/qrcode": "^0.3.2"
  }
}
